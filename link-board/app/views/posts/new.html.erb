<h1>Create a New Post</h1>
  <div class="row">
    <div class="col">
      <div>
        <%= form_for :post do |f| %>
        <%= f.text_field :title, placeholder: "title" %>
        <%= f.text_field :link, placeholder: "link" %>
        <%= f.hidden_field :user_id, value: @current_user.id %>
        <%= f.submit "Submit" %>
        <%end%>
      <div>
    </div>
</div>
